{
  "name": "solarwinds-apm",
  "version": "15.0.0",
  "description": "OpenTelemetry-based SolarWinds APM library",
  "license": "Apache-2.0",
  "contributors": [
    "Raphaël Thériault <raphael.theriault@solarwinds.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solarwinds/apm-js.git",
    "directory": "packages/solarwinds-apm"
  },
  "bugs": {
    "url": "https://github.com/solarwinds/apm-js/issues"
  },
  "keywords": [
    "tracing",
    "instrumentation",
    "performance",
    "monitoring",
    "profiling",
    "debugging",
    "analytics",
    "metrics",
    "apm",
    "otel",
    "opentelemetry",
    "telemetry",
    "solarwinds"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/commonjs/index.js"
    }
  },
  "main": "./dist/commonjs/index.js",
  "files": [
    "./src/",
    "./dist/",
    "./CHANGELOG.md",
    "./CONFIGURATION.md"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "build": "node build.js && tsc",
    "lint": "node build.js && prettier --check . && eslint . --max-warnings=0",
    "lint:fix": "node build.js && eslint --fix . && prettier --write .",
    "release": "node ../../scripts/publish.js",
    "scan": "node ../../scripts/scan.js",
    "test": "swtest -p test/tsconfig.json -c src"
  },
  "dependencies": {
    "@opentelemetry/api-logs": "~0.57.0",
    "@opentelemetry/core": "~1.30.0",
    "@opentelemetry/exporter-logs-otlp-proto": "~0.57.0",
    "@opentelemetry/exporter-metrics-otlp-proto": "~0.57.0",
    "@opentelemetry/exporter-trace-otlp-proto": "~0.57.0",
    "@opentelemetry/instrumentation": "~0.57.0",
    "@opentelemetry/resources": "~1.30.0",
    "@opentelemetry/sdk-logs": "~0.57.0",
    "@opentelemetry/sdk-metrics": "~1.30.0",
    "@opentelemetry/sdk-trace-base": "~1.30.0",
    "@opentelemetry/sdk-trace-node": "~1.30.0",
    "@opentelemetry/semantic-conventions": "~1.28.0",
    "@solarwinds-apm/bindings": "workspace:^",
    "@solarwinds-apm/dependencies": "workspace:^",
    "@solarwinds-apm/histogram": "workspace:^",
    "@solarwinds-apm/instrumentations": "workspace:^",
    "@solarwinds-apm/module": "workspace:^",
    "@solarwinds-apm/sampling": "workspace:^",
    "json-stringify-safe": "^5.0.1",
    "node-releases": "^2.0.18",
    "semver": "^7.6.3",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.9.0"
  },
  "peerDependenciesMeta": {
    "@opentelemetry/api": {
      "optional": false
    }
  },
  "devDependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@solarwinds-apm/eslint-config": "workspace:^",
    "@solarwinds-apm/test": "workspace:^",
    "@types/json-stringify-safe": "^5.0.3",
    "@types/node": "^18.19.0",
    "eslint": "^9.12.0",
    "prettier": "^3.3.3",
    "typescript": "~5.7.2"
  },
  "engines": {
    "node": "^18.19.0 || ^20.8.0 || >=22.0.0"
  }
}
