generator client {
  provider               = "prisma-client"
  output                 = "../app/db/"
  generatedFileExtension = "ts"
  importFileExtension    = "ts"
}

datasource db {
  provider = "sqlite"
}

model Item {
  id          Int     @id @default(autoincrement())
  description String
  done        Boolean @default(false)
}
